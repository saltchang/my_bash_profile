{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "console_title_template": "{{ .PWD }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#FF8700",
          "background": "#FFFFAF",
          "template": " {{ if .SSHSession }}\uf817 {{ end }}{{ .UserName }} ",
          "properties": {
            "display_host": false,
            "display_user": true
          }
        },
        {
          "type": "session",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#E99D2A",
          "background": "#1D485F",
          "template": " {{ if .SSHSession }}\uf817 {{ end }}{{ .HostName }} ",
          "properties": {
            "display_host": true,
            "display_user": false
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#00005F",
          "background": "#87D7D7",
          "template": " \uf07c  {{ .Path }} ",
          "properties": {
            "folder_separator_icon": "/",
            "style": "agnoster_full"
          }
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "node",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "#ffffff",
          "background": "#6CA35E",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "properties": {
            "display_mode": "environment"
          }
        },
        {
          "type": "dotnet",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "#e9e9e9",
          "background": "#512BD4",
          "template": " \ue77f {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "python",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "#e9e9e9",
          "background": "#346996",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "properties": {
            "display_mode": "environment"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#1D485F",
          "background": "#5FD787",
          "background_templates": [
            "{{ if or (or (gt .Working.Modified 0) (gt .Working.Deleted 0)) (.Staging.Changed) }}#FF8700{{ end }}"
          ],
          "template": "{{if .UpstreamIcon }} {{ .UpstreamIcon }}{{ end }}{{ .HEAD }}{{ if or (gt .Behind 0) (gt .Ahead 0) }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ end }}{{ if .Staging.Changed }} \uf055 {{ end }}{{ if or (gt .Working.Modified 0) (gt .Working.Deleted 0) }} \uf06a {{ end }}{{ if gt .Working.Untracked 0 }} \uf059 {{ end }} ",
          "properties": {
            "bitbucket_icon": "\uf171 ",
            "branch_ahead_icon": "\uf01b ",
            "branch_behind_icon": "\uf01a ",
            "branch_gone_icon": "\uf127",
            "branch_icon": " \uf126 ",
            "branch_identical_icon": "\uf102",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true,
            "git_icon": "\uf1d3",
            "github_icon": "\uf113 ",
            "gitlab_icon": "\uf296 "
          }
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "exit",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "#AFFF87",
          "foreground_templates": ["{{ if gt .Code 0 }}#D70000{{ end }}"],
          "background": "#5F5F5F",
          "template": " {{ if gt .Code 0 }}\uf00d{{ else }}\uf00c{{ end }} ",
          "properties": {
            "always_enabled": true
          }
        },
        {
          "type": "time",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "#1D485F",
          "background": "#D7AF5F",
          "template": " {{ .CurrentDate | date .Format }} \uf64f ",
          "properties": {
            "time_format": "15:04"
          }
        },
        {
          "type": "sysinfo",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "#54c0a5",
          "background": "#5F5F5F",
          "properties": {
            "postfix": "% ",
            "precision": 2,
            "prefix": " \uf85a "
          }
        },
        {
          "type": "shell",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "#ffffff",
          "background": "#0077c2",
          "template": " {{ .Name }} ",
          "properties": {
            "mapped_shell_names": {
              "pwsh": "posh"
            }
          }
        },
        {
          "type": "os",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "#EEEEEE",
          "background": "#121212",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "root",
          "style": "plain",
          "foreground": "#f1184c",
          "template": " \uf0e7 ",
          "properties": {
            "root_icon": "\u26a1"
          }
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#3cf118",
          "template": "\u2b9e "
        }
      ],
      "newline": true
    }
  ]
}
